{
  "name": "nextjs-blog",
  "private": true,
  "description": "A blog powered by Next.js and Markdown",
  "version": "2.0.0",
  "author": "Mika A. <@mikaoelitiana>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mikaoelitiana/gatsby-blog.git"
  },
  "scripts": {
    "dev": "next dev",
    "build": "tsx lib/copy-blog-images.ts && next build",
    "start": "next start",
    "lint": "eslint .",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,md,mdx}\"",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "date-fns": "^4.1.0",
    "gray-matter": "^4.0.3",
    "next": "^16.1.2",
    "react": "^19.0.0",
    "react-dom": "^19.2.3",
    "remark": "^15.0.1",
    "remark-html": "^16.0.1",
    "remark-prism": "^1.3.6",
    "sharp": "^0.34.5"
  },
  "devDependencies": {
    "@types/node": "^25.0.8",
    "@types/react": "^19.2.8",
    "@types/react-dom": "^19.0.2",
    "eslint": "^9.39.2",
    "eslint-config-next": "^16.1.1",
    "prettier": "^3.8.0",
    "tsx": "^4.21.0",
    "typescript": "^5.7.2"
  },
  "keywords": [
    "nextjs",
    "blog",
    "markdown",
    "typescript"
  ]
}
